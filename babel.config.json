{
    "presets": [
        "@babel/preset-react",
        ["@babel/preset-env" ,  { "modules": false }]
    ],
    "plugins": [
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      "@babel/plugin-syntax-dynamic-import",
      [
        "module-resolver", {
        "root": ["./src"]
      }],
      [
        "inline-react-svg"
      ]
      ]
}
